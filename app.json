{
  "expo": {
    "name": "moodfade",
    "slug": "moodfade",
    "version": "Beta",
    "orientation": "portrait",
    "scheme": "moodfade",
    "extra": {
      "redirectUri": "moodfade://redirect"
    },
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "infoPlist": {
        "UITextContentTypeUsername": "none",
        "UITextContentTypePassword": "none",
        "ASCredentialIdentityStoreEnabled": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-secure-store"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
